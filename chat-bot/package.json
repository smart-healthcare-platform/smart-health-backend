{
  "name": "chatbot-service",
  "version": "1.0.0",
  "description": "AI-powered healthcare chatbot service for cardiovascular patients",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "test": "jest --detectOpenHandles",
    "test:integration": "jest tests/integration/ --verbose --detectOpenHandles",
    "test:coverage": "jest --coverage --detectOpenHandles",
    "test:watch": "jest --watch --detectOpenHandles",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "format": "prettier --write src/"
  },
  "keywords": [
    "chatbot",
    "healthcare",
    "ai",
    "express"
  ],
  "author": "Chatbot Service Team",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.4.0",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.18.2",
    "express-rate-limit": "^6.8.1",
    "helmet": "^7.0.0",
    "joi": "^17.9.2",
    "lodash": "^4.17.21",
    "morgan": "^1.10.0",
    "mysql2": "^3.14.3",
    "redis": "^4.6.7",
    "sequelize": "^6.32.1",
    "winston": "^3.17.0",
    "chromadb": "^1.8.1",
    "chromadb-default-embed": "^2.14.0"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "eslint": "^8.45.0",
    "jest": "^29.6.1",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.0",
    "supertest": "^6.3.3"
  },
  "directories": {
    "doc": "docs",
    "test": "tests"
  }
}
